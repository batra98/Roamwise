# RoamWise Environment Variables
# Copy this file to .env and fill in your API keys

# EXA Search API Key (for web search)
EXA_API_KEY=your_exa_api_key_here

# Browserbase API Key (for web scraping)
BROWSERBASE_API_KEY=your_browserbase_api_key_here
BROWSERBASE_PROJECT_ID=your_browserbase_project_id_here

# Weave API Key (for logging)
WEAVE_API_KEY=your_weave_api_key_here

# Google Vertex AI API Key (for LLM)
GOOGLE_API_KEY=your_google_vertex_ai_key_here
