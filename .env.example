# RoamWise Environment Configuration
# Copy this file to .env and fill in your API keys
# Focus on core tools: Weave, Exa, Browserbase, Fly.io, Google A2A

# Exa API Configuration (Required for flight search)
EXA_API_KEY=your_exa_api_key_here

# Weave Configuration (Required for logging)
# WANDB_API_KEY is set in ~/.netrc
WANDB_PROJECT=gbatra3-uw-madison/Roamwise

# Browserbase Configuration (For hotel scraping)
BROWSERBASE_API_KEY=your_browserbase_api_key_here
BROWSERBASE_PROJECT_ID=your_browserbase_project_id_here

# Fly.io Configuration (For compute functions)
FLY_API_TOKEN=your_fly_api_token_here
FLY_APP_NAME=your_fly_app_name_here

# Google A2A Configuration (For advanced integrations)
GOOGLE_A2A_CLIENT_ID=your_google_a2a_client_id_here
GOOGLE_A2A_CLIENT_SECRET=your_google_a2a_client_secret_here

# Google Gemini API Configuration (For CrewAI LLM)
# Get your API key from: https://aistudio.google.com/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Google Vertex AI Configuration (For enhanced LLM features)
# Get your API key from: https://console.cloud.google.com/apis/credentials
GOOGLE_VERTEX_API_KEY=your_google_vertex_ai_api_key_here

# Application Settings
DEBUG=false
LOG_LEVEL=INFO
